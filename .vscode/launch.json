    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch GML Import",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "args": ["gml", "data/openroads/OSOpenRoads_TQ.gml"]
        },
        {
            "name": "Launch RefData Import",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "args": [
                "refdata",
                "road_classifications",
                "http://www.os.uk/xml/codelists/RoadClassificationValue.xml"
            ]
        }
    ]
}
